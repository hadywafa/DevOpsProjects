<!doctype html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>consumer-mvc</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" />
</head>

<body class="bg-light">
    <div class="container py-5">
        <header class="mb-4">
            <h1>consumer-mvc</h1>
            <p class="text-muted">ASP.NET Core MVC consuming <code>Company.Utils</code></p>
        </header>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        <footer class="pt-3 mt-4 text-muted border-top">
            &copy; @DateTime.Now.Year - consumer-mvc
        </footer>
    </div>
</body>

</html>
